{
  "$schema": "./appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "ContactMail": {
    "To": "john.doe@example.com",
    "ToName": "John Doe"
  },
  "DictionaryLocalization": {
    "DataAnnotationOnly": false,
    "DictionaryPrefix": ""
  },
  "Umbraco": {
    "CMS": {
      "ModelsBuilder": {
        "Enable": false,
        "ModelsMode": "Nothing"
      },
      "Hosting": {
        "Debug": false
      },
      "Global": {
        "Id": "abc1af5b-fe6d-4dfc-8fa5-f9bba72b01bf",
        "UseHttps": true,
        "SanitizeTinyMce": true
      },
      "Content": {
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      }
    }
  }
}